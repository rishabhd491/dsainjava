import java.util.*;

public class linaersearch2 {
    public static int main(int[] arr,int key){

        for(int i=0;i<arr.length;i++){
           if(arr[i]==key){
               return i+1;
           }

        }
        return -1;
    }
    public static void main(String args[]){
        int[] a1={20,30,40,70,60};
       int  key=40;
       System.out.println(key+"the position of the key is"+main( a1,key));

    }

}
